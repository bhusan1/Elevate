{
  "expo": {
    "name": "ElevateJuly",
    "slug": "ElevateJuly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ssp.Elevate",
      "buildNumber": "1",
      "usesIcloudStorage": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Elevate requires to enable Calendar usage"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ssp.elevate",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "notification": {
      "icon": "./src/assets/icon.png",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Unread Notifications"
    }
  }
}
